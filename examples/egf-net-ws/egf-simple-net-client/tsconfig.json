{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.proxy",
      "es2015",
      "es2016.array.include"
    ],
    "sourceMap": true,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "removeComments": false,
    "noEmitOnError": false,
    "outDir": "./bin",
    "types": [
      "https://cdn.jsdelivr.net/npm/@ailhc/enet@1.0.0/dist/es/types/index.d.ts"
    ]
  }
}