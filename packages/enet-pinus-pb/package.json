{
  "name": "@ailhc/enet-pinus-pb",
  "version": "0.1.0",
  "description": "基于enet.IProtoHandler的适用于Pinus或pomelo的前端协议处理器",
  "keywords": [
    "pinus",
    "pomelo",
    "enet",
    "socket",
    "protobuf",
    "protocol",
    "network"
  ],
  "author": "AILHC <505126057@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AILHC/EasyGameFrameworkOpen.git"
  },
  "exports": "./dist/es/index.mjs",
  "main": "dist/cjs/lib/index.js",
  "module": "dist/es/lib/index.mjs",
  "typings": "dist/cjs/types",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch:cjs": "egf build -w true -f cjs",
    "build:cjs": "egf build -f cjs",
    "build:es": "egf build -f es",
    "build:umd": "egf build -f umd",
    "build:iife": "egf build -f iife",
    "build:system": "egf build -f system:@ailhc/enet-pinus-pb",
    "clean-dist": "npx rimraf dist",
    "build:all": "npm run clean-dist && npm run build:cjs&&npm run build:es&&npm run build:umd&&npm run build:iife&&npm run build:system"
  },
  "bugs": {
    "url": "https://github.com/AILHC/EasyGameFrameworkOpen/issues"
  },
  "homepage": "https://github.com/AILHC/EasyGameFrameworkOpen/tree/main/packages/enet-pinus-pb#readme",
  "devDependencies": {
    "@ailhc/egf-cli": "^1.2.0",
    "@ailhc/enet": "^1.1.3"
  }
}
