{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.proxy",
      "es2015",
      "es2016.array.include"
    ],
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "removeComments": false,
    "noEmit": true
  }
}